   "source": [
    "class Solution:\n",
    "    def reverseWords(self, s: str) -> str:\n",
    "        out = []\n",
    "        for voca in s.split():\n",
    "            out.append(voca[::-1])\n",
    "        return ' '.join(out)"
   ]
