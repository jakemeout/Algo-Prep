   "source": [
    "class Solution:\n",
    "    def customSortString(self, order: str, s: str) -> str:\n",
    "        lst = list(s)\n",
    "        lst.sort(key=lambda char: order.find(char))\n",
    "        return ''.join(lst)        "
   ]
