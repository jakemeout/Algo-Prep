   "source": [
    "class Solution:\n",
    "    def arraySign(self, nums):\n",
    "        import numpy as np\n",
    "        x = np.array(nums)\n",
    "        x[x>0] = 1\n",
    "        x[x<0] = -1\n",
    "        return x.prod()"
   ]
